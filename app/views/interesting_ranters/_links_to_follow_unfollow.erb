<% interesting_ranter = kenny_loggins.interesting_ranters.find_by_person_id(@person_to_follow.id) %>
<% if interesting_ranter %>
  <%=link_to "Unfollow", "", class:"unfollower", 'data-interesting-ranter-id'=> interesting_ranter.id, 'data-ranter-id'=> @person_to_follow.id %>
<% else %>
  <%=link_to "Follow", "", class:"follower", 'data-interesting-ranter-id'=> "", 'data-ranter-id'=> @person_to_follow.id %>
<% end %>

