<% favorited_rant = kenny_loggins.favorites.find_by_rant_id(@rant_to_fave.id)%>

  <% if favorited_rant %>

  <% fave_count = favorited_rant.rant.favorites.count%>
  <% if fave_count == 0%>
    <%fave_count_for_link = ""%>
  <% else %>
    <%fave_count_for_link = "#{fave_count}-"%>
  <% end %>


  <%=link_to "#{fave_count_for_link}" + "Unfavorite", "", class:"right-floater unfavoriter", 'data-favorite-id'=>favorited_rant.id, 'data-rant-id'=> @rant_to_fave.id%>

<% else %>
  <%=link_to "#{fave_count_for_link}" + "Favorite", "", class:"right-floater favoriter", 'data-favorite-id'=>"", 'data-rant-id'=> @rant_to_fave.id%>

<% end %>

