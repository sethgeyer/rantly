<% favorite = Favorite.where(rant_id: @rant_to_fave.id, user_id: kenny_loggins.id).first %>
<% fave_count_for_link = Favorite.create_favorite_count(@rant_to_fave.id) %>

<% if favorite %>
  <div class="right-floater unfavoriter" data-favorite-id="<%=favorite.id%>" data-rant-id="<%=@rant_to_fave.id%>">
    <%="#{fave_count_for_link}" + "Unfavorite"%>
  </div>



<% else %>

  <div class="right-floater favoriter" data-rant-id="<%=@rant_to_fave.id%>" data-favorite-id="">
    <%="#{fave_count_for_link}" + "Favorite"%>
  </div>


<% end %>

